{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react-jsx",
    "types": ["node"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    ".storybook/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "storybook-static"
  ]
}
